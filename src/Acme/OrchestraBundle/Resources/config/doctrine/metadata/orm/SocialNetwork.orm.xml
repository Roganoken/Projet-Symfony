<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SocialNetwork" table="social_network">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="32" nullable="false"/>
    <field name="url" type="string" column="url" length="256" nullable="false"/>
    <field name="baseUrl" type="string" column="base_url" length="256" nullable="false"/>
    <field name="shareUrl" type="string" column="share_url" length="256" nullable="true"/>
    <field name="logo" type="string" column="logo" length="45" nullable="true"/>
  </entity>
</doctrine-mapping>
